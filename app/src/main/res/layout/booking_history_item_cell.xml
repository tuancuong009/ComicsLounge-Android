<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <LinearLayout
        android:id="@+id/main_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="?android:attr/selectableItemBackground"
        android:clickable="true"
        android:orientation="vertical"
        android:paddingLeft="25dp"
        android:paddingTop="15dp"
        android:paddingRight="25dp"
        android:paddingBottom="15dp">

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/bh_title_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:fontFamily="@font/roboto"
            android:lines="1"
            android:text="@string/loading_str"
            android:textColor="@color/black"
            android:textSize="@dimen/booking_his_cell_title_font_size" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/bh_date_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="10dp"
            android:fontFamily="@font/roboto"
            android:lines="1"
            android:text="@string/loading_str"
            android:textSize="@dimen/booking_his_cell_date_font_size" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/bh_orderid_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/booking_his_cell_margin_top"
            android:fontFamily="@font/roboto"
            android:lines="1"
            android:text="@string/loading_str"
            android:textSize="@dimen/booking_his_cell_date_font_size" />

        <androidx.appcompat.widget.AppCompatTextView
            android:id="@+id/bh_status_txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/booking_his_cell_margin_top"
            android:fontFamily="@font/roboto"
            android:lines="1"
            android:text="@string/loading_str"
            android:textSize="@dimen/booking_his_cell_date_font_size" />
    </LinearLayout>


</LinearLayout>